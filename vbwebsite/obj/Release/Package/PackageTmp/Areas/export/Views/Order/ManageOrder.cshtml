        function FinalTotal() {
            debugger;
            var TotalCarton = 0;
            var TotalQuantity = 0;
            var InvoiceTotalAmt = 0;
            $('#OrderQty').find('tr:visible').not(':last').each(function (index, tr) {
                $(tr).find('td').each(function (index, td) {
                    if (index == 5) {
                        var Carton = $(tr).find('input')[3].value;
                        if (Carton != '') {
                            TotalCarton = TotalCarton + parseFloat(Carton);
                            $('#TotalCarton').text(parseFloat(TotalCarton));
                            $('#TotalPkgs').val(parseFloat(TotalCarton));
                        }
                    }
                    else if (index == 6) {
                        var Quantity = $(tr).find('input')[7].value;
                        if (Quantity != '') {
                            TotalQuantity = TotalQuantity + parseFloat(Quantity);
                            $('#TotalQuantity').text(parseFloat(TotalQuantity));
                        }
                    }
                    else if (index == 8) {
                        if (td.innerText.trim() != '') {
                            InvoiceTotalAmt = parseFloat(InvoiceTotalAmt) + parseFloat(td.innerText);
                            $('#InvoiceTotalAmt').text(parseFloat(InvoiceTotalAmt).toFixed(2));
                            //$('#TextInvoiceTotalAmt').val(parseFloat(InvoiceTotalAmt).toFixed(2));
                            $('#GrandInvoiceTotalAmt').val((parseFloat(InvoiceTotalAmt) + parseFloat($('#InsuranceAmount').val()) + parseFloat($('#FreightAmount').val())).toFixed(2));
                        }
                    }
                });
            });
        }
    });


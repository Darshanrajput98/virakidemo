        data.TaxOnTotalIncome = $("#TaxOnTotalIncome").val();
        data.TaxOnTotalIncome_one = $("#TaxOnTotalIncome_one").val();
        data.RebateUnderSection87A_Income = $("#RebateUnderSection87A_Income").val();
        data.Surcharge = $("#Surcharge").val();
        data.Education = $("#Education").val();

        //Add By Dhruvik
        data.FormSixteenEHRA = $("#FormSixteenEHRA").val();
        //Add By Dhruvik

        $.ajax({
            url: '@Url.Action("PrintEmployeeForm16", "Report")',
            type: 'POST',
            //data: {
            //    'FromDate': FromDate, 'ToDate': ToDate, 'EmployeeCode': EmployeeCode, 'StandardDeduction': StandardDeduction,
            //    'HousingLoanPrincipal': HousingLoanPrincipal, 'ELSS': ELSS, 'PPF': PPF,

@*@model List<vb.data.retproductqtyviewmodel>*@
@model List<vb.Data.RetProductQtyViewModel>

@{
    ViewBag.Title = "ProductCategoryList";
    Layout = "~/Areas/retail/Views/Shared/_retaillayout.cshtml";
}

<div class="box">
    <div class="box-header text-center">
        <h3 class="box-title">Product List</h3>
    </div>
    <div class="box-footer ">
        <div class="pull-right">
            <button type="button" id="btnsavecase" class="btn btn-info">Save</button>
        </div>
    </div>
    <div class="box-body table-scrollable1">
        <table id="ListDataTable" class="table table-bordered table-bordered table-striped lst1">
            <thead>
                <tr>
                    <th class="nosort">Name</th>
                    <th class="nosort">Price</th>
                    <th class="nosort">MRP</th>
                    <th class="nosort">Barcode</th>
                    <th class="nosort">Size</th>
                    <th>Print Barcode</th>
                    <th>Ingredients & Nutrition Value</th>
                    <th>Print Blank Barcode</th>
                    <th>Print Label</th>
                    <th>Single Barcode</th>
                    <th>External Barcode</th>
                    <th>Wholesale Barcode</th>
                    <th>Pouch Barcode</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td id="row">
                            @item.ProductName
                            <input class="ProductQtyID" type="hidden" value="@item.ProductQtyID">
                        </td>
                        <td>
                            <input class="form-control ProductPrice" type="text" name="SGST" style="width:120px" value="@item.ProductPrice" />
                        </td>
                        <td>
                            <input class="form-control ProductMRP" type="text" name="CGST" style="width:120px" value="@item.ProductMRP" />
                        </td>
                        <td>
                            <input class="form-control ProductBarcode" type="text" disabled="disabled" name="IGST" style="width:120px" value="@item.ProductBarcode" />
                        </td>
                        <td>
                            @item.PouchSize
                        </td>
                        <td>
                            <a style="cursor:pointer;" id="lnkbarcode" class="btnbarcode" data-ppname="@item.ProductName" data-pmrp="@item.ProductMRP" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-nutritionvalue="@item.NutritionValue" data-contentvalue="@item.ContentValue" data-categoryid="@item.CategoryID" data-productqtyid="@item.ProductQtyID" data-gramperkg="@item.GramPerKG">Print Barcode</a>
                        </td>
                        <td>
                            @if (item.NutritionValue == "" && item.ContentValue == "")
                            {
                                <input class="chkContent" type="checkbox" placeholder="Content" disabled="disabled" />
                            }
                            else
                            {
                                <input class="chkContent" type="checkbox" placeholder="Content" checked="checked" disabled="disabled" />
                            }
                        </td>
                        <td>
                            <a style="cursor:pointer;" id="lnkbarcode1" class="btnblankbarcode" data-ppname="@item.ProductName" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-nutritionvalue="@item.NutritionValue" data-contentvalue="@item.ContentValue" data-categoryid="@item.CategoryID" data-productqtyid="@item.ProductQtyID" data-gramperkg="@item.GramPerKG">Blank Barcode</a>
                        </td>
                        <td><a style="cursor:pointer;" id="lnklabel" class="btnlabel" data-lpname="@item.ProductName" data-pid="@item.ProductID">Print Label</a></td>
                        <td>
                            <a style="cursor:pointer;" class="btnsinglebarcode" data-ppname="@item.ProductName" data-pmrp="@item.ProductMRP" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-nutritionvalue="@item.NutritionValue" data-contentvalue="@item.ContentValue" data-categoryid="@item.CategoryID" data-productqtyid="@item.ProductQtyID" data-gramperkg="@item.GramPerKG">Single Barcode</a>
                        </td>
                        <td>
                            <a style="cursor:pointer;" id="lnkbarcode1" class="btnexternalbarcode" data-productqtyid="@item.ProductQtyID" data-ppname="@item.ProductName" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-nutritionvalue="@item.NutritionValue" data-contentvalue="@item.ContentValue" data-categoryid="@item.CategoryID" data-placeoforigin="@item.PlaceOfOrigin" data-gramperkg="@item.GramPerKG">External Barcode</a>
                        </td>
                        <td>
                            <a style="cursor:pointer;" id="lnkwholesalebarcode" class="btnwholesalebarcode" data-ppname="@item.ProductName" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-categoryid="@item.CategoryID" data-productmrp="@item.ProductMRP" data-productquantity="@item.ProductQuantity" data-unit="@item.UnitName" data-gramperkg="@item.GramPerKG">Wholesale Barcode</a>
                        </td>
                        <td>
                            <a style="cursor:pointer;" id="lnkpouchbarcode" class="btnpouchbarcode" data-ppname="@item.ProductName" data-pbarcode="@item.ProductBarcode" data-pid="@item.ProductID" data-categoryid="@item.CategoryID" data-productmrp="@item.ProductMRP" data-productquantity="@item.ProductQuantity" data-unit="@item.UnitName" data-nutritionvalue="@item.NutritionValue" data-contentvalue="@item.ContentValue" data-productqtyid="@item.ProductQtyID" data-pouchsize="@item.PouchSize"
                               data-protein="@item.Protein" data-fat="@item.Fat" data-carbohydrate="@item.Carbohydrate" data-totalenergy="@item.TotalEnergy" data-information="@item.Information" data-pouchname="@item.PouchName" data-gramperkg="@item.GramPerKG">Pouch Barcode</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

        <div id="responsive" class="modal fade">
            <div class="modal-dialog">

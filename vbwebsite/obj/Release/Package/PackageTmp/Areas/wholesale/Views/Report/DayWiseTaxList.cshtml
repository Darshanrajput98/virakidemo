@model List<vb.Data.DayWiseTaxList>

<div class="box">
    <div class="box-header text-center">
        <h3 class="box-title">Tax Wise Summary</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body table-scrollable1">
        <table id="ListDataTable" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="nosort" style="text-align:center">Gross Amt Total</th>
                    <th class="nosort" colspan="2" style="text-align:center">CGST</th>
                    <th class="nosort" colspan="2" style="text-align:center">SGST</th>
                    <th class="nosort" colspan="2" style="text-align:center">IGST</th>
                    <th class="nosort" style="text-align:center">Tax Amt Total</th>
                    <th class="nosort" style="text-align:center">TCS</th>
                    <th class="nosort" style="text-align:center">Round Off Total</th>
                    <th class="nosort" style="text-align:center">Net Amt Total</th>
                </tr>
                <tr>
                    <th></th>
                    <th style="text-align:center">Tax %</th>
                    <th style="text-align:center">Tax Amt</th>
                    <th style="text-align:center">Tax %</th>
                    <th style="text-align:center">Tax Amt</th>
                    <th style="text-align:center">Tax %</th>
                    <th style="text-align:center">Tax Amt</th>
                    <th></th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.GrossAmtTotal
                        </td>
                        <td>
                            @item.CGST
                        </td>
                        <td>
                            @item.TaxAmtCGST
                        </td>
                        <td>
                            @item.SGST
                        </td>
                        <td>
                            @item.TaxAmtSGST
                        </td>
                        <td>
                            @item.IGST
                        </td>
                        <td>
                            @item.TaxAmtIGST
                        </td>
                        <td>
                            @item.TaxAmtTotal
                        </td>
                        <td>
                            @item.TCSAmtTotal
                        </td>
                        <td>
                            @item.RoundOffTotal
                        </td>
                        <td>
                            @item.NetAmtTotal
                        </td>
                    </tr>
                }
                @if (Model.Count > 0)
                {
                    <tr style="font-weight:bold">
                        <td>
                            Total : @Model[0].GrandGrossAmtTotal
                        </td>

                        <td colspan="2" style="text-align:right">
                            @Model[0].GrandCGSTAmt
                        </td>
                        <td colspan="2" style="text-align:right">
                            @Model[0].GrandSGSTAmt
                        </td>
                        <td colspan="2" style="text-align:right">
                            @Model[0].GrandIGSTAmt
                        </td>
                        <td>@Model[0].GrandTaxAmtTotal</td>
                        <td>@Model[0].GrandTCSAmtTotal</td>
                        <td>@Model[0].GrandRoundOffTotal</td>
                        <td>@Model[0].GrandNetAmtTotal</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <!-- /.box-body -->
</div>

<script>
</script>




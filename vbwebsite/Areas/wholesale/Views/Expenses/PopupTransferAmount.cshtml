@model List<vb.Data.TransferAmountListResponse>
<div class="box">
    <!-- /.box-header -->
    <div class="box-body">
        <table id="tblListDataTablewithourpaging" class="table table-bordered table-striped lst1">
            <thead>
                <tr>
                    <th class="nosort">From</th>
                    <th class="nosort">To</th>
                    <th class="nosort">Amount</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            <input type="hidden" class="hdnTransferID" value="@item.TransferID" />
                            @item.FromGodownName
                        </td>
                        <td>
                            @item.ToGodownName
                        </td>
                        <td>
                            @item.Amount.ToString("0.00")
                        </td>
                    </tr>
                }

            </tbody>
        </table>
        <div class="modal-footer">
            @*<button type="button" class="btnupdate btn btn-info">Update</button>*@
            <button type="button" class="btnclose btn btn-info" data-dismiss="modal" aria-hidden="true">Close</button>
        </div>
    </div>
</div>

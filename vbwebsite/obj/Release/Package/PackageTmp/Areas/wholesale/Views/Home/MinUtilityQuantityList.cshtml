<div class="box">
    <div class="box-header text-center">
        <h3 class="box-title">Min Utility Quantity</h3>
    </div>
    <div class="box-body table-scrollable1">
        <table id="ListDataTable2" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="nosort">Utility Name</th>
                    <th class="nosort">Utility Description</th>
                    <th class="nosort">Min Utility Qty.</th>
                    <th class="nosort">Godown  </th>
                    <th class="nosort">Opening Utility</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.UtilityName
                        </td>
                        <td>
                            @item.UtilityDescription
                        </td>
                        <td>
                            @item.MinUtilityQuantity
                        </td>
                        <td>
                            @item.GodownName
                        </td>
                        <td>
                            @item.OpeningUtility
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
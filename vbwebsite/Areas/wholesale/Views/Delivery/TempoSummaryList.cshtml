@model List<vb.Data.TempoSummary>
@{
    ViewBag.Title = "TempoSummaryList";
    Layout = "~/Areas/wholesale/Views/Shared/_wholesalelayout.cshtml";
}

<div class="box">
    <div id="divprint">
        <div class="box-header text-center">
            <h3 class="box-title">Tempo Summary</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body table-scrollable1">
            <table id="ListDataTableNoInfo" class="table table-bordered table-striped">
                <tbody>
                    <tr>
                        @foreach (var item in Model)
                        {
                            <td>
                                @MvcHtmlString.Create(item.VehicleSummary.Replace(Environment.NewLine, "<br/>"))
                            </td>
                        }
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="box-footer ">
        <div class="pull-right">
            <button type="button" id="btnprint" class="btn btn-info" tabindex="6">Print</button>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script>
    $(document).ready(function () {
        $("#btnprint").click(function () {
            debugger;
            var url = '@Url.Action("ExportExcelTempoSummary", "Delivery")';
            window.location.href = url;
        });
    });
</script>
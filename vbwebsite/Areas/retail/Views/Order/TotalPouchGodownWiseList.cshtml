@model List<vb.Data.TotalPouchListGodownWise>
@using vb.Data.ViewModel

<div class="box">
    <div class="box-header text-center">
        <h3 class="box-title">Total Pouch List</h3>
    </div>
    <div class="box-body table-scrollable1">
        <table id="ListDataTableNoPaging1" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="nosort">Godown</th>
                    <th class="nosort">Pouch Size</th>
                    <th class="nosort">Quantity</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.GodownName
                        </td>
                        <td>
                            @item.PouchSize
                        </td>
                        <td>
                            @item.QuantityPackage
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


